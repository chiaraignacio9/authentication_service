{
    "name": "chiaraignacio9/authenticatio_service",
    "description": "authentication service following SOLID principles, patterns as repository, adapter",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"            
        },
        "classmap": [
            "src/presentation/*",
            "src/infrastructure/*",
            "src/domain/*",
            "src/config/envs.plugin.php",
            "src/config/jwt.plugin.php"
        ]
    },
    "require": {
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    }
}
